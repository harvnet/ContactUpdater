<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.harv0116.midterm" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <icon density="mdpi" src="www/icons/plogo_mdpi_48x48.png" />
    <icon density="hdpi" src="www/icons/plogo_hdpi_72x72.png" />
    <icon density="xhdpi" src="www/icons/plogo_xhdpi_96x96.png" />
    <icon density="xxhdpi" src="www/icons/plogo_xxhdpi.png" />
    <splash density="land-hdpi" src="www/splash/splashscreen_800x480land.png" />
    <splash density="land-ldpi" src="www/splash/splashscreen_320x200land.png" />
    <splash density="land-mdpi" src="www/splash/splashscreen_480x320land.png" />
    <splash density="land-xhdpi" src="www/splash/splashscreen_1280x720land.png" />
    <splash density="port-hdpi" src="www/splash/splashscreen_480x800port.png" />
    <splash density="port-ldpi" src="www/splash/splashscreen_200x320port.png" />
    <splash density="port-mdpi" src="www/splash/splashscreen_320x480port.png" />
    <splash density="port-xhdpi" src="www/splash/splashscreen_720x1280port.png" />
    <name>Contact Updater</name>
    <description>
        A program that does geolocation and contacts
    </description>
    <author email="harv0116@algonquinlive.com">
        Paul Harvey
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
